CREATE TABLE Users(
    userID varchar(255) NOT NULL,
    password varchar(255) NOT NULL,
    chatId varchar(255),
    PRIMARY KEY (userID)
);

CREATE TABLE Chats(
    chatID varchar(255) NOT NULL,
    userID varchar(255) NOT NULL,
    Messages LONGTEXT,
    Prompt varchar(8000),
    summary varchar(1000) NOT NULL,
    PRIMARY KEY (chatID)
);

CREATE TABLE Emails(
    emailID varchar(200) NOT NULL
);